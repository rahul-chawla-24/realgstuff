{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "cd client && npm start",
    "start": "nodemon ./bin/www",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "dev": "concurrently -n 'server,client' -c 'red,green' \"npm start\" \"npm run client\"",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.6",
    "validator": "^13.0.0"
  }
}
